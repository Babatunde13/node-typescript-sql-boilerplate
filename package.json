{
   "name": "node-boiler-plate-sql",
   "version": "1.0.0",
   "description": "Boilerplate for Node.js SQL, built using ExpressJS, TypeScript, and TypeOrm. Built with ❤️ by @bkoiki950",
   "main": "index.js",
   "scripts": {
      "test": "mocha dist/**/*.spec.js",
      "lint": "eslint --ext .js,.ts src/",
      "build": "tsc --project ./tsconfig.json",
      "start": "npm run db:upgrade && node dist/src/index.js",
      "dev": "nodemon dist/src/index.js",
      "db:migrate": "typeorm migration:generate -n",
      "db:upgrade": "typeorm migration:run",
      "db:downgrade": "typeorm migration:revert",
      "prepare": "husky install"
   },
   "keywords": [],
   "author": "Babatunde Koiki",
   "license": "ISC",
   "devDependencies": {
      "@types/axios": "0.14.0",
      "@types/bcrypt": "^5.0.0",
      "@types/chai": "4.3.0",
      "@types/dotenv": "8.2.0",
      "@types/express": "4.17.13",
      "@types/jsonwebtoken": "8.5.7",
      "@types/mocha": "9.0.0",
      "@types/morgan": "1.9.3",
      "@types/node": "17.0.8",
      "@types/pg": "8.6.4",
      "@typescript-eslint/eslint-plugin": "5.9.1",
      "@typescript-eslint/parser": "5.9.1",
      "chai": "4.3.4",
      "eslint": "8.6.0",
      "husky": "7.0.4",
      "mocha": "9.1.4",
      "nodemon": "2.0.15",
      "superagent": "7.0.2",
      "typescript": "4.5.4"
   },
   "dependencies": {
      "axios": "0.24.0",
      "bcrypt": "^5.0.1",
      "dotenv": "12.0.4",
      "express": "4.17.2",
      "morgan": "1.10.0",
      "pg": "8.7.1",
      "reflect-metadata": "0.1.10",
      "typeorm": "0.2.41"
   }
}
